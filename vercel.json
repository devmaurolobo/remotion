{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/videos/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/api/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
