{
  "version": 2,
  "builds": [
    {
      "src": "api/vercel-simple.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/vercel-simple.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "REMOTION_CACHE_DIR": "/tmp",
    "REMOTION_OUTPUT_DIR": "/tmp"
  }
}
