{
  "version": 2,
  "builds": [
    {
      "src": "api/vercel-programmatic.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/vercel-programmatic.js"
    },
    {
      "src": "/videos/(.*)",
      "dest": "/api/vercel-programmatic.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/api/vercel-programmatic.js"
    }
  ],
  "env": {
    "REMOTION_CACHE_DIR": "/tmp",
    "REMOTION_OUTPUT_DIR": "/tmp"
  }
}
